{
    "name": "grocery",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "main": "bun start",
    "engines": {
        "bun": "1.2.X",
        "node": "22.X"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/notkimroberts/grocery-program"
    },
    "scripts": {
        "build": "bun --bun run vite build",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "clean": "rm -rf .svelte-kit && rm -rf build && rm -rf node_modules/.vite",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:nuke": "",
        "db:push": "drizzle-kit push",
        "db:start": "docker compose up",
        "db:studio": "drizzle-kit studio",
        "dev": "bun --bun run vite dev",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "prepare": "svelte-kit sync || echo ''",
        "preview": "vite preview",
        "test:e2e": "playwright test",
        "test:unit": "vitest",
        "test": "bun run test:unit -- --run && bun run test:e2e"
    },
    "devDependencies": {
        "@eslint/compat": "^1.3.2",
        "@eslint/js": "^9.34.0",
        "@internationalized/date": "^3.8.1",
        "@lucide/svelte": "^0.515.0",
        "@playwright/test": "^1.55.0",
        "@sveltejs/adapter-vercel": "^5.10.2",
        "@sveltejs/kit": "^2.37.0",
        "@sveltejs/vite-plugin-svelte": "^6.1.3",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/vite": "^4.1.12",
        "@types/node": "22.12.0",
        "@vitest/browser": "^3.2.4",
        "bits-ui": "^2.8.6",
        "clsx": "^2.1.1",
        "drizzle-kit": "^0.31.4",
        "eslint": "^9.34.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-svelte": "^3.11.0",
        "globals": "^16.3.0",
        "playwright": "^1.55.0",
        "prettier": "^3.6.2",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "svelte": "^5.38.6",
        "svelte-check": "^4.3.1",
        "tailwind-merge": "^3.3.1",
        "tailwind-variants": "^1.0.0",
        "tailwindcss": "^4.1.12",
        "tw-animate-css": "^1.3.7",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.41.0",
        "vite": "^7.1.3",
        "vite-plugin-devtools-json": "^1.0.0",
        "vitest": "^3.2.4",
        "vitest-browser-svelte": "^1.1.0"
    },
    "dependencies": {
        "@neondatabase/serverless": "^1.0.1",
        "@node-rs/argon2": "^2.0.2",
        "@oslojs/crypto": "^1.0.1",
        "@oslojs/encoding": "^1.1.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/debug": "^4.1.12",
        "@types/lodash-es": "^4.17.12",
        "debug": "^4.4.1",
        "drizzle-orm": "^0.44.5",
        "drizzle-seed": "^0.3.1",
        "lodash-es": "^4.17.21",
        "mode-watcher": "^1.1.0",
        "postgres": "^3.4.7"
    }
}
